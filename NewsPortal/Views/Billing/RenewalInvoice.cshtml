@model IEnumerable<News.Model.BillingModel>
@{

    ViewBag.Title = "UserLogin";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<section class="content-header">
    <h1>
        User List
    </h1>
    <ol class="breadcrumb">
   
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Renewal Date</th>
                                <th>Attachment</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>@Html.DisplayFor(modelItem => item.Id)</td>
                                <td>@Html.DisplayFor(modelItem => item.BillingDate)</td>
                                <td>@Html.DisplayFor(modelItem => item.InvoiceAttachment)</td>
                                <td>
                                    <a href="@item.InvoiceAttachment" download>Download</a>
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>


